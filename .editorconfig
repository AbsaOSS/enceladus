# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf

[*.xml]
indent_size = 4
indent_style = tab

[*.{java,scala,js,json}]
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
