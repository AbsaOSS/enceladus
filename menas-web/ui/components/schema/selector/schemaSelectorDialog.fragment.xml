<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt"
    xmlns:cust="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <SelectDialog class="hAuto maxH90pc minW30rem" id="schemaSelectorSelectDialog" noDataText="No schemas found" title="Select Schema" search="components.schema.selector.SchemaSelectorUtils.onSearch"
